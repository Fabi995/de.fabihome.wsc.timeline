<?xml version="1.0" encoding="UTF-8"?>
<package name="de.fabihome.wcf.timeline" xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com https://www.woltlab.com/XSD/maelstrom/package.xsd">
	<packageinformation>
		<packagename><![CDATA[Timeline]]></packagename>
		<packagedescription><![CDATA[Timeline for successes or something else.]]></packagedescription>
		<packagedescription language="de"><![CDATA[Timeline für Erfolge oder Anderes.]]></packagedescription>
		<version>1.0.0</version>
		<date>2017-05-11</date>
		<license><![CDATA[All rights reserved <https://fabihome.de/license.html>]]></license>
	</packageinformation>

	<authorinformation>
		<author>Fabian (Fabi_995), Morik, Mysterycode</author>
		<authorurl>https://fabihome.de</authorurl>
	</authorinformation>

	<requiredpackages>
		<requiredpackage minversion="2.1.0">com.woltlab.wcf</requiredpackage>
		<requiredpackage minversion="1.1.2 pl 1" file="requirements/de.codequake.wysiwyg.acp.tar">de.codequake.wysiwyg.acp</requiredpackage>
	</requiredpackages>

	<excludedpackages>
		<excludedpackage version="3.0.0 Alpha 1">com.woltlab.wcf</excludedpackage>
	</excludedpackages>

	<instructions type="install">
		<!-- files -->
		<instruction type="file">files.tar</instruction>
		<instruction type="template">templates.tar</instruction>
		<instruction type="acpTemplate">acpTemplates.tar</instruction>

		<!-- xml -->
		<instruction type="userGroupOption">userGroupOption.xml</instruction>
		<instruction type="pageMenu">pageMenu.xml</instruction>
		<instruction type="acpMenu">acpMenu.xml</instruction>
		<instruction type="option">option.xml</instruction>

		<!-- db -->
		<instruction type="sql">install.sql</instruction>

		<!-- language -->
		<instruction type="language">language/*.xml</instruction>
	</instructions>
</package>
